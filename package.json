{
    "name": "min-web-dev",
    "author": {
        "name": "Adam R Moss",
        "email": "adamrmoss@gmail.com"
    },
    "description": "An example résumé developed as a minimalist website",
    "version": "1.0.0",
    "homepage": "https://github.com/adamrmoss/min-web-dev",
    "repository": {
        "type": "git",
        "url": "https://github.com/adamrmoss/min-web-dev.git"
    },
    "license": "MIT",
    "devDependencies": {
        "fs": "x",
        "glob": "10.3.x",
        "sass": "1.70.x",
        "ts-node": "10.x",
        "typescript": "5.x"
    },
    "scripts": {
        "build": "dotnet build min-web-dev.sln",
        "build-sass": "npx sass MinWebDev.Web/Sources/Resume/resume.scss MinWebDev.Web/wwwroot/site.css",
        "init-settings": "npx ts-node initialize-settings.ts",
        "publish": "dotnet publish MinWebDev.Web -o publish",
        "start": "npm run build && dotnet run --project MinWebDev.Web"
    }
}
